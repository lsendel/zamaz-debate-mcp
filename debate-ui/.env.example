# Copy this file to .env.local and customize as needed

# Port for the development server (default: 3000)
PORT=3000

# API URL (usually not needed due to proxy)
REACT_APP_API_URL=http://localhost:3000

# WebSocket URL (usually not needed due to proxy)
REACT_APP_WS_URL=ws://localhost:5013

# Enable debug logging
REACT_APP_DEBUG=false