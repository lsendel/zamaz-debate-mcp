# UI Environment Variables Example
# Copy this file to .env and adjust values as needed

# UI Server Port
VITE_PORT=3001

# API Endpoints (adjust these based on your backend deployment)
VITE_ORGANIZATION_API_URL=http://localhost:5005
VITE_LLM_API_URL=http://localhost:5002
VITE_DEBATE_API_URL=http://localhost:5013
VITE_RAG_API_URL=http://localhost:5004

# WebSocket URL for real-time updates
VITE_WS_URL=ws://localhost:5013

# CORS Origins (comma-separated list)
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# API Base Path
VITE_API_BASE_PATH=/api

# Development Mode
VITE_DEVELOPMENT_MODE=true

# Feature Flags
VITE_ENABLE_MOCK_AUTH=false
VITE_ENABLE_DEBUG_LOGGING=true