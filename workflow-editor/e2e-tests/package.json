{
  "name": "workflow-editor-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Kiro Workflow Editor",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:reports": "playwright test tests/reports/ --reporter=html",
    "test:dashboards": "playwright test tests/dashboards/ --reporter=html",
    "install-browsers": "playwright install"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "workflow",
    "telemetry",
    "reports"
  ],
  "author": "Zamaz Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}